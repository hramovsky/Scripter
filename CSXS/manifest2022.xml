	<ExtensionList>
		<Extension Id="com.scripter.panel" Version="1.0" />
	</ExtensionList>

	<ExecutionEnvironment>
	<HostList>
        <!-- AfterEffects -->
        <Host Name="AEFT" Version="[15.0,25.9]" />

	</HostList>
	<LocaleList>
		<Locale Code="All" />
	</LocaleList>
	<RequiredRuntimeList>
		<RequiredRuntime Name="CSXS" Version="7.0" />
	</RequiredRuntimeList>
</ExecutionEnvironment>

	<DispatchInfoList>
