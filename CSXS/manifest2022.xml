			</Resources>

			<Lifecycle>
				<AutoVisible>true</AutoVisible>
			</Lifecycle>

			<UI>

				<Type>Panel</Type>
				<Menu>Scripter</Menu>

				<Geometry>
					<Size>
						<Height>565</Height>
						<Width>490</Width>
					</Size>
					<MinSize>
						<Height>565</Height>
						<Width>490</Width>
					</MinSize>
					<MaxSize>
						<Height>565</Height>
						<Width>490</Width>
					</MaxSize>

				</Geometry>

			</UI>
		</DispatchInfo>
	</Extension>
</DispatchInfoList>
</ExtensionManifest>
